  m_sTextData "1x New Takistan Army rifle platoon located east of Zarifkheyl."\
  ""\
  "Wearing lizard camo, equipped with donated deprecated western equipment (M16A2s, FN MAGs)."
  m_aVisibleForFactions {
   "GC_INSURGENT"
  }
  m_iOrder 4
