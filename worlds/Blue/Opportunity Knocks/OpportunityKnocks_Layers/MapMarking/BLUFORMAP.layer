$grp PS_MissionDescription : "{3136BE42592F3B1B}PrefabsEditable/MissionDescription/EditableMissionDescription.et" {
 BRIEF1 {
  coords 2837.862 55.942 1382.461
  m_sTitle "1. SITUATION"
  m_sTextData "a. Enemy Forces"\
  "A Russian artillery battery (up to three 2S1 Gvozdika SP guns) has recently arrived in the area and appears lightly prepared. Possible ENY Motorized QRF in the area ready to support."\
  ""\
  "b. Friendly Forces"\
  "White Platoon, White Company — conducting a night patrol. No higher echelon attachments or detachments for this mission. Neighboring friendly elements maintain sector security but cannot provide immediate direct fire support inside the objective area."\
  ""\
  "c. Attachments / Detachments"\
  "None."
  m_aVisibleForFactions {
   "RHS_USAF"
  }
 }
 BRIEF2 {
  coords 2840.718 56.062 1389.911
  m_sTitle "2. MISSION"
  m_sTextData "White Platoon will seize the opportunity destroy as many 2S1 guns, neutralize enemy crews, and withdraw in good order to friendly lines."
  m_aVisibleForFactions {
   "RHS_USAF"
  }
  m_iOrder 1
 }
 BRIEF3 {
  coords 2844.655 56.104 1394.242
  m_sTitle "3. EXECUTION"
  m_sTextData "Commander's Intent"\
  "Exploit surprise to eliminate the artillery threat with speed and violence of action, while preserving platoon combat power sufficient for withdrawal. "
  m_aVisibleForFactions {
   "RHS_USAF"
  }
  m_iOrder 2
 }
 BRIEF4 {
  coords 2852.697 56.125 1394.889
  m_sTitle "4. SERVICE SUPPORT"
  m_sTextData "a. Logistics"\
  ""\
  "Ammo: Platoon carries organic resupply only; no scheduled vehicle resupply inside the AO. 8x DEMO Blocks provided in crate infront of Platoon HQ"\
  ""\
  "Medical: On-site CLS."\
  ""\
  "Recovery: No vehicle recovery assets available inside AO."\
  ""\
  "b. Sustainment Notes"\
  ""\
  "Batteries for NVGs and comms to be checked before movement."
  m_aVisibleForFactions {
   "RHS_USAF"
  }
  m_iOrder 3
 }
 BRIEF5 {
  coords 2856.074 56.156 1387.511
  m_sTitle "5. COMMAND & SIGNAL"
  m_sTextData "a. Command"\
  ""\
  "Platoon Commander: CO - WHITE 6"\
  ""\
  "Succession: CO → Platoon Sergeant (1st XO) → Squad 1 Leader."\
  ""\
  "b. Signal"\
  ""\
  "Primary Net: CH48"\
  ""\
  "Tac Callsigns: "\
  "PL - WHITE 6"\
  "PSG - WHITE 7"\
  "1st/2nd/3rd/4th SQUAD - WHITE 1/2/3/4"\
  ""\
  "Pyro/IR: Use IR strobes for identification only when required to avoid alerting nearby enemy patrols."
  m_aVisibleForFactions {
   "RHS_USAF"
  }
  m_iOrder 4
 }
 BRIEF6 {
  coords 2859.165 56.006 1378.365
  m_sTitle "6. WARNING & NOTES"
  m_sTextData "Night operations increase risk of fratricide — positive ID and clear fire sectors are mandatory."\
  ""\
  "Maintain noise and light discipline; NVGs only when required."\
  ""\
  "Speed and surprise are decisive — do not allow the enemy time to mass indirect fires."
  m_aVisibleForFactions {
   "RHS_USAF"
  }
  m_iOrder 5
 }
}
