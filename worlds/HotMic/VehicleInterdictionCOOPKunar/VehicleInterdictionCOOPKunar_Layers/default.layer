TILW_MissionFrameworkEntity fw1 : "{8F846D0FD5D6EA51}Prefabs/MP/TILW_MissionFramework.et" {
 coords 2682.254 8 3053.212
 userScript "	// code here"\
 "	IEntity BlueSupplyDepot;"\
 " 	SCR_ResourceComponent BlueRC;"\
 " 	SCR_ResourceContainer container;"
 FrameworkScript1 ""\
 "		// Set the Supplies value on random chosen van.\""\
 "		"\
 "		// Get the active flag name from random flag list"\
 "		string cargoVehicleName = \"\";"\
 "		if (IsMissionFlag(\"PickConvoyAFlag\")) cargoVehicleName = \"CargoA\";"\
 "		if (IsMissionFlag(\"PickConvoyBFlag\")) cargoVehicleName = \"CargoB\";"\
 "		if (IsMissionFlag(\"PickConvoyCFlag\")) cargoVehicleName = \"CargoC\";"\
 "		"\
 "		if (cargoVehicleName == \"\")"\
 "		{"\
 "			Print(\"Failed to acquired the name for which Cargo Van in the world\");"\
 "			RaiseFailedFlag();"\
 "		} else "\
 "			PrintFormat(\"FW1 picked %1 as a winner!\", cargoVehicleName);"\
 "		"\
 "		// Use that name to get the vehicle in the Game World"\
 "		IEntity cargoVan = GetGame().GetWorld().FindEntityByName(cargoVehicleName);"\
 "		if (!cargoVan)"\
 "		{"\
 "			PrintFormat(\"No entity of %1 found\", cargoVehicleName);"\
 "			RaiseFailedFlag();"\
 "		}"\
 "		"\
 "		// Get the Resource Component, then Resource Container, and then set the value"\
 "		SCR_ResourceComponent cargoVanRC = SCR_ResourceComponent.FindResourceComponent(cargoVan, true);"\
 "		SCR_ResourceContainer container;"\
 "		"\
 "		if (cargoVanRC && cargoVanRC.GetContainer(EResourceType.SUPPLIES, container))"\
 "		{"\
 "  			container.SetResourceValue(250);"\
 "			Print(\"FW1 Script ran successfully\");"\
 "		} else {"\
 "			PrintFormat(\"%1 did not have either a resource component or resource container\", cargoVehicleName);"\
 "			RaiseFailedFlag();"\
 "		}"\
 "	}"\
 "		"\
 "	private void RaiseFailedFlag()"\
 "	{"\
 "		Print(\"FW1 Script soft failed\");"\
 "		AdjustMissionFlag(\"Failed Cargo Van Supplies Flag\", true);"\
 "		return;"\
 "	"
 FrameworkScript2 ""\
 "		// Check the Current Supply value of Supply Depot with Target value(250)"\
 "		"\
 "		float currentSuppliesValue = container.GetResourceValue();"\
 "		if (currentSuppliesValue == 250) "\
 "		{"\
 "			AdjustMissionFlag(\"Check Supplies Success Flag\", true);"\
 "		} else {"\
 "			PrintFormat(\"FW2 Check fail | %1 < 250\", currentSuppliesValue);"\
 "		}"\
 "	"
 FrameworkScript3 ""\
 "		// Set the Variables that will be use in Framework Script 2"\
 " 		"\
 " 		BlueSupplyDepot = GetGame().GetWorld().FindEntityByName(\"Bluefor_Supply_Depot\");"\
 "		"\
 "		if (!BlueSupplyDepot)"\
 " 		{"\
 "			Print(\"No entity of Bluefor Supply Depot found\");"\
 " 			RaiseFailedFlag3();"\
 " 		}"\
 "			"\
 "		BlueRC = SCR_ResourceComponent.FindResourceComponent(BlueSupplyDepot, true);"\
 "		if (!(BlueRC && BlueRC.GetContainer(EResourceType.SUPPLIES, container)))"\
 " 		{"\
 "			Print(\"Bluefor Supply Depot did not have either a resource component or resource container\");"\
 " 			RaiseFailedFlag3();"\
 " 		}"\
 "			"\
 " 		Print(\"FW3 Script ran successfully\");"\
 " 	}"\
 "			"\
 " 	private void RaiseFailedFlag3()"\
 "	{"\
 " 		Print(\"FW3 Script soft failed\");"\
 " 		return;"\
 "	"
